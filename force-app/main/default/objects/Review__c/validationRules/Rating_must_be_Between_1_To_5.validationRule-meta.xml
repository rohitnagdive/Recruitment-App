<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Rating_must_be_Between_1_To_5</fullName>
    <active>true</active>
    <errorConditionFormula>Rating__c &lt;1  ||  Rating__c &gt;10</errorConditionFormula>
    <errorDisplayField>Rating__c</errorDisplayField>
    <errorMessage>Rating should be in Between 1 to 10</errorMessage>
</ValidationRule>
